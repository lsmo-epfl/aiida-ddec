{
  "name": "aiida-ddec",
  "version": "1.0.0a1",
  "license": "MIT License",
  "author": "Aliaksandr Yakutovich",
  "author_email": "aliaksandr.yakutovich@epfl.ch",
  "description": "AiiDA plugin DDEC code",
  "url": "https:\/\/github.com\/yakutovicha\/aiida-ddec",
  "classifiers": [
    "License :: OSI Approved :: MIT License",
    "Programming Language :: Python :: 2.7",
    "Programming Language :: Python :: 3.6",
    "Programming Language :: Python :: 3.7",
    "Development Status :: 3 - Alpha"
  ],
  "entry_points": {
    "aiida.calculations": [
      "ddec = aiida_ddec.calculations:DdecCalculation"
    ],
    "aiida.parsers": [
      "ddec = aiida_ddec.parsers:DdecParser"
    ],
    "aiida.workflows": [
      "ddec.cp2k_ddec = aiida_ddec.workchains:Cp2kDdecWorkChain"
    ]
  },
  "setup_requires": [
    "reentry"
  ],
  "data_files": [
    [
      ".",
      [
        "setup.json"
      ]
    ]
  ],
  "reentry_register": true,
  "install_requires": [
    "aiida_core >= 1.0.0b6",
    "six"
  ],
  "extras_require": {
    "cp2k":[
      "aiida-cp2k>=1.0.0b4"
    ],
    "pre-commit": [
      "pre-commit==1.17.0",
      "yapf==0.28.0",
      "prospector==1.1.7",
      "pylint==1.9.4; python_version<'3.0'",
      "pylint==2.3.1; python_version>='3.0'"
    ],
    "testing": [
      "pytest==4.4.1"
    ]
  }
}
